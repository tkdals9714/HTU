<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%>
<%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags" %>
<script src="/resources/js/jquery.min.js"></script>
<style>
	.crewHeaderName{font-size: 20px; padding-bottom:30px;}
	.CrewTable{width: 100%; height: 50%;}
	.crewTable-tr{border-top: 1px solid gray; padding: 3px;}
</style>
<sec:authentication property="principal.userVO" var="userVO"/>
<div class="crewHeaderName">
	나의 동아리
</div>
<table class="CrewTable">
	<tr class="table_title">
		<td>동아리명</td>
		<td>동아리장</td>
		<td>위치</td>
	</tr>
	<c:forEach var="crewVO" items="${myCrew}">
		<tr class="crewTable-tr">
			<td><a href="/student/crew/crewDetail?crCd=${crewVO.crCd}">${crewVO.crNm}</a></td>
			<td>${crewVO.ldrNm}</td>
			<td>학생회관 ${crewVO.roomNm}</td>
		</tr>
	</c:forEach>
</table>
