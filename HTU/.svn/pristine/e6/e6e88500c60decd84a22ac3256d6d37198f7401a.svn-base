<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%>
<%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags"%>
<style>
	.report_box{width:100%; padding:40px; background-color:#ffffff;}
	.report_box .table_topic{font-size: 20px; padding-bottom:30px;}
	.report_box .table_topic i{margin-left:10px;}
	.report_box table{font-size:14px; width:100%; text-align:center;}
	.table_title{font-size:13px; border-top:1px solid #d1d0d0; border-bottom:1px solid #d1d0d0; height:32px; line-height:32px;}
	.report_box tr{height: 40px;}
	.report_box tr td:nth-child(1){width:5%;}
	.report_box tr td:nth-child(2){width:20%;}
	.report_box tr td:nth-child(3){width:20%;}
	.report_box tr td:nth-child(4){width:20%;}
	.report_box tr td:nth-child(5){width:35%;}

</style>
<div  class="report_box" style="background-color: white;">
<div style="font-size:12px; color:#888888; margin-bottom: 20px;">
		<span><svg xmlns="http://www.w3.org/2000/svg" height="13" width="13" viewBox="0 0 576 512" style="margin-right: 5px;"><path fill="#888888" d="M575.8 255.5c0 18-15 32.1-32 32.1h-32l.7 160.2c0 2.7-.2 5.4-.5 8.1V472c0 22.1-17.9 40-40 40H456c-1.1 0-2.2 0-3.3-.1c-1.4 .1-2.8 .1-4.2 .1H416 392c-22.1 0-40-17.9-40-40V448 384c0-17.7-14.3-32-32-32H256c-17.7 0-32 14.3-32 32v64 24c0 22.1-17.9 40-40 40H160 128.1c-1.5 0-3-.1-4.5-.2c-1.2 .1-2.4 .2-3.6 .2H104c-22.1 0-40-17.9-40-40V360c0-.9 0-1.9 .1-2.8V287.6H32c-18 0-32-14-32-32.1c0-9 3-17 10-24L266.4 8c7-7 15-8 22-8s15 2 21 7L564.8 231.5c8 7 12 15 11 24z"/></svg></span>
		<span style="font-size:12px; color:#888888;">></span>
		<span style="font-size:12px; color:#888888;">커뮤니티</span>
		<span style="font-size:12px; color:#888888;">></span>
		<span style="font-size:12px; color:#888888;">게시글 신고</span>
	</div>
	<div class="table_topic">신고 목록<i class="fa-solid fa-circle-info" style="color: #333333;"></i></div>
			<table>
				<tr class="table_title">
					<td>NO</td>
					<td>게시글번호</td>
					<td>신고사유</td>
					<td>신고일</td>
					<td>처리상황</td>
				</tr>
				<c:forEach items="${data.content}" var="postReportVO" varStatus="stat">
					<tr>
						<td>${postReportVO.rnum}</td>
						<td><a class="detail" href="/common/board/
							<c:if test="${postReportVO.boardVO.comdCd eq 'BORD01'}">free</c:if>
							<c:if test="${postReportVO.boardVO.comdCd eq 'BORD02'}">publicize</c:if>
							<c:if test="${postReportVO.boardVO.comdCd eq 'BORD03'}">anonymous</c:if>
								/detail?bdCd=${postReportVO.bdCd}"  data-lecapp-code="${postReportVO.bdCd}">${postReportVO.bdCd}</a>
						</td>
						<td>
							 <c:choose>
						        <c:when test="${postReportVO.prRsn eq 0}">불건전한 내용</c:when>
						        <c:when test="${postReportVO.prRsn eq 1}">주제에 맞지 않음</c:when>
						        <c:when test="${postReportVO.prRsn eq 2}">욕설 및 혐오표현</c:when>
						        <c:when test="${postReportVO.prRsn eq 3}">기타</c:when>
						    </c:choose>
						</td>
                  		<td>
                  			<fmt:formatDate value="${postReportVO.approvalVO.appReg}" pattern="yyyy-MM-dd"/>
                  		</td>
						<td>
							<c:if test="${postReportVO.approvalVO.appYn eq 0}"><button class="btn-two mini green">결재대기</button></c:if>
							<c:if test="${postReportVO.approvalVO.appYn eq 1}"><button class="btn-two mini blue">결재완료</button></c:if>
							<c:if test="${postReportVO.approvalVO.appYn eq 2}"><button class="btn-two mini red">반려</button></c:if>
						</td>
               		</tr>
            	</c:forEach>
      </table>
   </div>
   <div>${data.getPagingArea()}</div>

<script>

</script>

